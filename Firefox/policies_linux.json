{
    "policies": {
        "CaptivePortal": false,
        "Cookies": {
            "Behavior": "reject-foreign"
        },
        "DisableAccounts": true,
        "DisableFirefoxAccounts": true,
        "DisableFirefoxScreenshots": true,
        "DisableFirefoxStudies": true,
        "DisableFormHistory": true,
        "DisablePocket": true,
        "DisableTelemetry": true,
        "DontCheckDefaultBrowser": true,
        "EnableTrackingProtection": {
            "Value": true,
            "Cryptomining": true,
            "Fingerprinting": true,
            "EmailTracking": true
        },
        "EncryptedMediaExtensions": {
            "Enabled": false
        },
        "ExtensionSettings": {
            "*": {
                "install_sources": [
                    "about:addons",
                    "https://addons.mozilla.org/"
                ]
            },
            "uBlock0@raymondhill.net": {
                "installation_mode": "force_installed",
                "install_url": "https://addons.mozilla.org/firefox/downloads/latest/ublock-origin/latest.xpi",
                "private_browsing": true
            }
        },
        "FirefoxHome": {
            "Search": true,
            "TopSites": true,
            "SponsoredTopSites": false,
            "Highlights": false,
            "Pocket": false,
            "SponsoredPocket": false,
            "Snippets": true
        },
        "HardwareAcceleration": true,
        "HttpsOnlyMode": "force_enabled",
        "NetworkPrediction": false,
        "NoDefaultBookmarks": true,
        "OfferToSaveLogins": false,
        "OverrideFirstRunPage": "",
        "OverridePostUpdatePage": "",
        "PasswordManagerEnabled": false,
        "PDFjs": {
            "Enabled": true,
            "EnablePermissions": false
        },
        "Permissions": {
            "Camera": {
                "BlockNewRequests": false,
                "Locked": false
            },
            "Microphone": {
                "BlockNewRequests": false,
                "Locked": false
            },
            "Location": {
                "BlockNewRequests": true,
                "Locked": true
            },
            "Notifications": {
                "BlockNewRequests": false,
                "Locked": false
            },
            "Autoplay": {
                "Allow": [
                    "https://twitch.tv"
                ],
                "Default": "block-audio-video",
                "Locked": false
            }
        },
        "Preferences": {
            "browser.backspace_action": {
                "Value": 0,
                "Status": "default"
            },
            "browser.contentblocking.category": {
                "Value": "strict",
                "Status": "user"
            },
            "browser.region.update.enabled": {
                "Value": false,
                "Status": "user"
            },
            "browser.region.network.url": {
                "Value": "",
                "Status": "user"
            },
            "browser.startup.homepage_override.mstone": {
                "Value": "ignore",
                "Status": "user"
            },
            "browser.vpn_promo.enabled": {
                "Value": false,
                "Status": "user"
            },
            "browser.cache.disk.enable": {
                "Value": false,
                "Status": "default"
            },
            "browser.cache.disk_cache_ssl": {
                "Value": false,
                "Status": "default"
            },
            "privacy.globalprivacycontrol.enabled": {
                "Value": true,
                "Status": "user"
            },
            "media.ffmpeg.vaapi.enabled": {
                "Value": true,
                "Status": "user"
            },
            "extensions.blocklist.enabled": {
                "Value": false,
                "Status": "user"
            },
            "extensions.getAddons.cache.enabled": {
                "Value": false,
                "Status": "user"
            },
            "extensions.webextensions.restrictedDomains": {
                "Value": "",
                "Status": "user"
            },
            "network.connectivity-service.enabled": {
                "Value": false,
                "Status": "user"
            }
        },
        "StartDownloadsInTempDirectory": true,
        "SearchEngines": {
            "Add": [
                {
                    "Name": "ArchLinux User Repository",
                    "URLTemplate": "https://aur.archlinux.org/packages?K={searchTerms}",
                    "Method": "GET",
                    "IconURL": "https://archlinux.org/static/favicon.png",
                    "Alias": "alaur"
                },
                                {
                    "Name": "ArchLinux Packages",
                    "URLTemplate": "https://archlinux.org/packages/?q={searchTerms}",
                    "Method": "GET",
                    "IconURL": "https://archlinux.org/static/favicon.png",
                    "Alias": "alpkgs"
                },
                                {
                    "Name": "ArchLinux Wiki",
                    "URLTemplate": "https://wiki.archlinux.org/index.php?search={searchTerms}",
                    "Method": "GET",
                    "IconURL": "https://archlinux.org/static/favicon.png",
                    "Alias": "alwiki"
                },
                {
                    "Name": "ArchLinux Manual Pages",
                    "URLTemplate": "https://man.archlinux.org/search?q={searchTerms}",
                    "Method": "GET",
                    "IconURL": "https://archlinux.org/static/favicon.png",
                    "Alias": "alman"
                },
                {
                    "Name": "ArchLinux PKGBUILD",
                    "URLTemplate": "https://gitlab.archlinux.org/archlinux/packaging/packages/{searchTerms}/-/blob/main/PKGBUILD?ref_type=heads",
                    "Method": "GET",
                    "IconURL": "https://archlinux.org/static/favicon.png",
                    "Alias": "alsrc"
                }
            ],
            "Default": "DuckDuckGo",
            "PreventInstalls": true,
            "Remove": [
                "Bing",
                "eBay",
                "Ecosia",
                "LEO Eng-Deu"
            ]
        },
        "TranslateEnabled": false,
        "UserMessaging": {
            "ExtensionRecommendations": false,
            "FeatureRecommendations": false,
            "UrlbarInterventions": false,
            "SkipOnboarding": false,
            "MoreFromMozilla": false,
            "FirefoxLabs": true,
            "Locked": true
        }
    }
}
